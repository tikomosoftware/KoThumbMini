<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="DropZoneTitle">画像をドロップ</system:String>
    <system:String x:Key="DropZoneSubTitle">ファイルまたはフォルダ</system:String>
    <system:String x:Key="LabelTargetSize">リサイズ後のサイズ</system:String>
    <system:String x:Key="StatusReady">準備完了</system:String>
    <system:String x:Key="StatusCollecting">ファイルを収集中...</system:String>
    <system:String x:Key="StatusNoImages">対象の画像が見つかりません</system:String>
